<%-include('../partials/head') %>
 <link rel="stylesheet" href="/home.css">
    </head>
<body>
    <header>
        <%-include('../partials/nav') %>
    </header>
    <main>
        <h1>Here our Registered Homes: </h1>
        
        <ul>
            <%registerHomes.forEach(home=>{ %>
                
                <li>
                    <img  class="new-image"  src="<%=home.photo %>"  width="1000"/>
                </li>
                <li>
                    <%=home.homeName %>
                </li>
                 <li>
                    <%=home.price %>
                </li>
                 <li>
                    <%=home.location %>
                </li>
                 <li class="card-bottom">
                    <a href="/homes/<%=home._id %>">Details</a>
                    <button>Book Now</button>
                </li>
                 
                 
            <%}) %>
        </ul>
       
        
    </main>
    
    
</body>
</html>